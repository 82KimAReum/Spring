<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
	
	<bean id="cp" class="com.javasoft.ConsolePrinter"></bean>
	<bean id="sp" class="com.javasoft.StringPrinter"></bean>
	<bean id="hello" class="com.javasoft.Hello">
		<constructor-arg index="0" value="박지민"></constructor-arg>
		<constructor-arg index="1" ref="sp"></constructor-arg>
	</bean>
	

</beans>
